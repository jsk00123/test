<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="test">

	<select id="boardList" parameterType="Map" resultType="HashMap">
		SELECT
			*
		FROM
			Trainee_SNS.Board
		WHERE
			1=1
	</select>

</mapper>